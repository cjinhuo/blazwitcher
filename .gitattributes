# Default to LF for all text files
* text=auto eol=lf

# Windows batch files
*.cmd text eol=crlf
*.bat text eol=crlf

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.svg binary
*.webp binary
*.pdf binary
*.zip binary
*.tar binary
*.gz binary
*.7z binary
*.rar binary
*.exe binary
*.dll binary
*.so binary
*.dylib binary
*.pyc binary
*.class binary
*.jar binary
*.ttf binary
*.otf binary
*.woff binary
*.woff2 binary
*.eot binary
*.mp3 binary
*.mp4 binary
*.mov binary
*.sqlite binary
*.db binary
